{
  "manifest_version": 3,
  "name": "GitHub accurate Time Display",
  "description": "A chrome-extension to show the exact timedelta on github",
  "version": "1.2.1",
  "icons": {
    "128": "clock.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}
